  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite --host",
    "start": "vite preview",
    "build-github": "vite build --config vite-github.config.ts",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write '{public,src}/**/*.{css,html,ts,js,svelte}'",
    "lint": "eslint --fix --quiet src --ext .ts,.js,.svelte",
    "types": "rm -rf $(find ./types/ -name \"*\" ! -name \"package.json\" ! -name \"types\") && tsc --project tsconfig.types.json"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.49",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "dot": "^1.1.3",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "node-sass": "^7.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.52.3",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.2",
    "svelte-eslint-parser": "^0.16.3",
    "svelte-feather-icons": "^4.0.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.12"
  },
