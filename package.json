{
  "name": "proto-kb",
  "version": "0.0.1",
  "description": "Alternative to Keyboard Layout Editor",
  "source": "src/index.html",
  "repository": "https://github.com/KB-Sanity/proto-kb.git",
  "author": "i7d3v3l0p3r@yandex.ru",
  "license": "MIT",
  "packageManager": "yarn@3.1.0",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "vite --config vite.config.js --host 0.0.0.0",
    "build": "tsc & vite build",
    "build-github": "tsc & vite build --config vite-github.config.js",
    "lint": "eslint '*/**/*.{js,ts}' --quiet --fix",
    "format": "prettier --write .",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint"
    ]
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.0",
    "@parcel/transformer-typescript-tsc": "^2.0.0",
    "@parcel/validator-typescript": "^2.0.0",
    "@types/feather-icons": "^4.7.0",
    "@types/offscreencanvas": "^2019.6.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "pinst": "^2.1.6",
    "posthtml-include": "^1.7.2",
    "precise-commits": "^1.0.2",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4",
    "vite": "^2.6.7",
    "vite-plugin-posthtml": "^0.0.3"
  },
  "dependencies": {
    "@pixi-essentials/svg": "^1.1.5",
    "colord": "^2.9.1",
    "feather-icons": "^4.28.0",
    "nanoevents": "^6.0.2",
    "nanoid": "^3.1.30",
    "nanostores": "^0.4.9",
    "pixi.js": "^6.1.3"
  }
}
