    "svelte-check": "^2.7.2",
    "svelte-eslint-parser": "^0.16.3",
    "svelte-feather-icons": "^4.0.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "colord": "^2.9.2",
    "nanoid": "^4.0.0",
    "nanostores": "^0.5.12",
    "pixi": "^0.3.1",
    "pixi.js": "^6.4.2",
    "sirv-cli": "^2.0.2"
