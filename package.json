    "colord": "^2.9.2",
    "git-url-parse": "^12.0.0",
    "nanoid": "^4.0.0",
    "nanostores": "^0.5.12",
    "pixi.js": "^6.4.2",
    "sirv-cli": "^2.0.2",
    "svelte-modals": "^1.1.0"
  },
  "packageManager": "yarn@3.2.1"
}
